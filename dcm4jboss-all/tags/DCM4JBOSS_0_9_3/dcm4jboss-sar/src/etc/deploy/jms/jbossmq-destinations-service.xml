<server>
  <mbean code="org.jboss.mq.server.jmx.Queue"
	 name="dcm4chex:service=Queue,name=MoveScu">
     <depends optional-attribute-name="DestinationManager">
     	jboss.mq:service=DestinationManager
     </depends>
  </mbean>  

  <mbean code="org.jboss.mq.server.jmx.Queue"
	 name="dcm4chex:service=Queue,name=IANScu">
     <depends optional-attribute-name="DestinationManager">
     	jboss.mq:service=DestinationManager
     </depends>
  </mbean>  

  <mbean code="org.jboss.mq.server.jmx.Queue"
	 name="dcm4chex:service=Queue,name=MPPSScu">
     <depends optional-attribute-name="DestinationManager">
     	jboss.mq:service=DestinationManager
     </depends>
  </mbean>  

</server>
